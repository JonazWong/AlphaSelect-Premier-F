# AlphaSelect Premier F - 環境變數配置

# ========================================
# 數據庫配置
# ========================================
POSTGRES_PORT=5432
DATABASE_URL=Update DATABASE_URL with user and default password

# ========================================
# Redis 配置
# ========================================
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# ========================================
# 服務端口
# ========================================
BACKEND_PORT=8000
FRONTEND_PORT=3000

# ========================================
# MEXC API 配置
# ========================================
# 🔑 請填入您的 MEXC API 憑證
# 獲取方式: https://www.mexc.com/user/openapi
MEXC_API_KEY=
MEXC_SECRET_KEY=

# MEXC API Base URLs（通常不需要修改）
MEXC_CONTRACT_BASE_URL=https://contract.mexc.com
MEXC_SPOT_BASE_URL=https://api.mexc.com

# ========================================
# 安全配置
# ========================================
# JWT Secret Key（用於用戶認證）
SECRET_KEY=Zy-K)eDk_iXw2.2qLjm/C!Tk!HW"w"'_LHPIe]Pr7F>Gd+9`Kx

# 允許的來源（CORS）
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# ========================================
# AI 模型配置
# ========================================
AI_MODEL_DIR=./ai_models

# ========================================
# 應用配置
# ========================================
APP_NAME=AlphaSelect Premier F
APP_VERSION=2.0.0
DEBUG=true

# ========================================
# 日誌配置
# ========================================
LOG_LEVEL=INFO

# ========================================
# WebSocket 配置
# ========================================
WEBSOCKET_VERBOSE_LOGGING=true

# ========================================
